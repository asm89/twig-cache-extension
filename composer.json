    "require": {
        "php": ">=5.3.2",
        "symfony/event-dispatcher": "~2.0",
        "twig/twig": "1.*"
    },
    "require-dev": {
        "doctrine/cache": "1.*",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": { "Asm89\\Twig\\CacheExtension\\": "lib/" }
