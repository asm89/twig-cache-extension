{
    "name": "asm89/twig-cache-extension",
    "description": "Caching template fragments with Twig.",
    "keywords": ["twig", "cache"],
    "homepage": "https://github.com/asm89/twig-cache-extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander",
            "email": "iam.asm89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "twig/twig": "1.*"
    },
    "require-dev": {
        "doctrine/common":          "~2.2",
        "zendframework/zend-cache": "~2.0",
        "zf1/zend-cache":           "1.12"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type":"package",
            "package": {
                "name": "zf1/zend-cache",
                "version": "1.12",
                "source": {
                    "url": "http://framework.zend.com/svn",
                    "type": "svn",
                    "reference": "framework/standard/trunk/library/Zend/Cache/"
                },
                "target-dir": "Zend/Cache",
                "autoload": {
                    "psr-0": { "Zend_Cache": "/" }
                }
            }
        }
    ],
    "autoload": {
        "psr-0": { "Asm89\\Twig\\CacheExtension\\": "lib/" }
    }
}
